package br.com.teste.json;

public class JsonCreate {
	
	public String generate(String nome,int codigo, String categoria,
			int preco){
		//JSONObject json;
		String jsonString = null;
		
		JsonObject json = json.createObjectBuilder()
				.add("nome",nome)
				.build();
		
		
		
		jsonString = json.toString();
	
		
		return jsonString;
	}

}
